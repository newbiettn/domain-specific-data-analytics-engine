                        </ToolBar>
                        <ToolBar prefHeight="39.0" prefWidth="639.0">
                            <items>
                                <Button fx:id="variableBtn" mnemonicParsing="false" onAction="#addVariableNode" text="Variable" />
                            </items>
                        </ToolBar>
                  <ToolBar prefHeight="40.0" prefWidth="200.0">
                    <items>
