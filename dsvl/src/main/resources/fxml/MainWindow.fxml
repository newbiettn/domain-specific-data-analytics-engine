<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<HBox prefHeight="680.0" prefWidth="1353.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainController">
    <children>
        <VBox minHeight="600" minWidth="400" prefHeight="680.0" prefWidth="1141.0">
            <children>
                <HBox prefHeight="41.0" prefWidth="1157.0" style="-fx-background-color: gray;" VBox.vgrow="ALWAYS">
                    <children>
                        <ToolBar prefHeight="41.0" prefWidth="370.0">
                            <items>
                                <Button fx:id="selectBtn" mnemonicParsing="false" onAction="#addSelectNode" text="Select" />
                                <Button fx:id="askBtn" mnemonicParsing="false" onAction="#addAskNode" text="Ask" />
                                <Button fx:id="createMLModelBtn" mnemonicParsing="false" onAction="#addCreateMLModelNode" text="Create ML Model" />
                            </items>
                        </ToolBar>
                        <ToolBar prefHeight="41.0" prefWidth="353.0">
                            <items>
                                <Button fx:id="patientBtn" mnemonicParsing="false" onAction="#addPatientNode" text="Patient" />
                                <Button fx:id="episodeBtn" mnemonicParsing="false" onAction="#addEpisodeNode" text="Episode" />
                            </items>
                        </ToolBar>
                        <ToolBar prefHeight="41.0" prefWidth="566.0">
                            <items>
                                <Button fx:id="variableBtn" mnemonicParsing="false" onAction="#addVariableNode" text="Variable" />
                            </items>
                        </ToolBar>
                    </children>
                </HBox>
                <AnchorPane prefHeight="643.0" prefWidth="1141.0" style="-fx-background-color: rgb(74, 75, 78);">
                    <StackPane fx:id="contentPane" alignment="BASELINE_CENTER" layoutX="24.0" layoutY="21.0" prefHeight="600.0" prefWidth="1093.0" styleClass="vflow-background" VBox.vgrow="ALWAYS">
                  <effect>
                     <DropShadow color="#464646" spread="0.84" />
                  </effect></StackPane>
                </AnchorPane>
            </children>
            <stylesheets>
                <URL value="@css/default.css" />
            </stylesheets>
        </VBox>
        <VBox prefHeight="680.0" prefWidth="213.0" style="-fx-background-color: gray;">
         <children>
            <Accordion prefHeight="688.0" prefWidth="95.0">
              <panes>
                <TitledPane animated="false" text="untitled 1">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                  </content>
                </TitledPane>
              </panes>
            </Accordion>
         </children></VBox>
    </children>
</HBox>
