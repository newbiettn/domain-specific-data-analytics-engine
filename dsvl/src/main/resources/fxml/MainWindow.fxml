<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="600" minWidth="400" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainController">
   <children>
      <HBox prefHeight="51.0" prefWidth="1288.0" style="-fx-background-color: gray;" VBox.vgrow="ALWAYS">
         <children>
            <ToolBar prefHeight="48.0" prefWidth="255.0">
              <items>
                  <Button fx:id="selectBtn" mnemonicParsing="false" onAction="#addSELECTNode" text="Select" />
                  <Button fx:id="askBtn" mnemonicParsing="false" onAction="#addASKNode" text="Ask" />
                  <Button fx:id="createMLModelBtn" mnemonicParsing="false" onAction="#addCREATEMLMODELNode" text="Create ML Model" />
              </items>
            </ToolBar>
            <ToolBar prefHeight="48.0" prefWidth="31.0" />
            <ToolBar prefHeight="48.0" prefWidth="266.0">
               <items>
                  <Button fx:id="patientBtn" mnemonicParsing="false" onAction="#addPATIENTNode" text="Patient" />
               </items>
            </ToolBar>
            <ToolBar prefHeight="48.0" prefWidth="760.0" />
         </children>
      </HBox>
      <StackPane fx:id="contentPane" alignment="BASELINE_CENTER" prefHeight="1126.0" prefWidth="1288.0" styleClass="vflow-background" VBox.vgrow="ALWAYS" />
      <AnchorPane prefHeight="200.0" prefWidth="200.0" />
   </children>
    <stylesheets>
        <URL value="@css/default.css" />
    </stylesheets>
</VBox>
