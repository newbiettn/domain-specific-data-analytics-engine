<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<HBox fx:id="nodeHboxContainer" styleClass="hbox-node" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UsePredictiveModelNodeController">
   <children>
      <BorderPane fx:id="nodeBorderPaneContainer" HBox.hgrow="ALWAYS">
         <center>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="USE LEARNING ALGORITHM" textAlignment="CENTER" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <ChoiceBox fx:id="cbLearningAlgorithmNames" prefWidth="150.0" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <stylesheets>
      <URL value="@css/default.css" />
   </stylesheets>
</HBox>
