<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<HBox fx:id="nodeHboxContainer"
      xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controllers.VariableNodeController">
   <children>
      <BorderPane fx:id="nodeBorderPaneContainer" HBox.hgrow="ALWAYS">
         <center>
            <TextField fx:id="variableNodeTextField"
                       BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <stylesheets>
      <URL value="@css/default.css" />
   </stylesheets>
</HBox>
